{
  "name": "client-ui-transformer",
  "version": "1.0.0",
  "description": "Transform flowscript UI code defined in researcher-ui into ES2015 for use in a mobile client webview.",
  "main": "index.js",
  "scripts": {
    "forever": "forever dist/client-ui-transformer.js",
    "flow": "flow",
    "build": "flow && webpack --config webpack.config.js  --progress --colors --bail",
    "test": "mocha --compilers js:node_modules/babel-core/register --harmony -R spec -t 5000 --sort test/*.spec.js"
  },
  "author": "Anders Borch",
  "license": "MIT",
  "dependencies": {
    "amqplib": "^0.5.1",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-tcomb": "^0.3.24",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-plugin-transform-function-bind": "^6.5.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babelify": "^7.3.0",
    "browserify": "^13.3.0",
    "flow-bin": "^0.37.4",
    "forever": "^0.15.3",
    "jsdom": "^9.9.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "nodemon": "^1.11.0",
    "path": "^0.12.7",
    "tcomb": "^3.2.15",
    "webpack": "^1.12.14",
    "winston": "^2.3.1"
  }
}
